# Backend Environment Variables
# Copy this file to .env and fill in your values

# Server Configuration
PORT=3001
NODE_ENV=development

# KICK OAuth2 Credentials
# Get these from https://kick.com/developer/applications
KICK_CLIENT_ID=your_kick_client_id_here
KICK_CLIENT_SECRET=your_kick_client_secret_here
KICK_REDIRECT_URI=http://localhost:3001/api/auth/callback

# Cookie Secret (generate a random string)
COOKIE_SECRET=your_random_secret_key_change_this_in_production

# Frontend URL
FRONTEND_URL=http://localhost:5173
